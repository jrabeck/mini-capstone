  <%= form_tag "/albums", method: :post do %>
    <div>
    <%= label_tag :name %>
    <%= text_field_tag :name %>
    <%= label_tag :email %>
    <%= text_field_tag :email %>
    <%= label_tag :phone %>
    <%= text_field_tag :phone %>
    <div>
    <%= submit_tag "Submit" %>
    </div>
    <% end %>